<div class="new_family_form">
  <%= form_for @family do |f| %>
    <%= f.label :parent_first_name %>
    <%= f.text_field :parent_first_name %>
    <br>
    <%= f.label :parent_last_name %>
    <%= f.text_field :parent_last_name %>
    <br>
    <%= f.label :spouse_name %>
    <%= f.text_field :spouse_name %>
    <br>
    <%= f.label :phone %>
    <%= f.text_field :phone %>
    <br>
    <%= f.label :address %>
    <%= f.text_field :address %>
    <br>
    <%= f.submit %>
  <% end %>
</div>