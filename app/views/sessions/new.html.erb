<h1>Please sign in:</h1>

<div id="family_log_in_form">
	<%= form_tag sign_in_path do %>
	  <%= label_tag :parent_last_name %>
	  <%= text_field_tag :parent_last_name, params[:parent_last_name] %>
	  <br>
		<%= label_tag :password %>
		<%= password_field_tag :password %>
	  <br>
		<%= submit_tag "Sign In" %>
	<% end %>
</div>